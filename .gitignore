# Create a comprehensive .gitignore
cat > .gitignore << 'EOF'
# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
share/python-wheels/
*.egg-info/
.installed.cfg
*.egg
MANIFEST

# Virtual Environments
venv/
env/
ENV/
.env/
.venv/

# Jupyter
.ipynb_checkpoints
*.ipynb_checkpoints

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS
.DS_Store
Thumbs.db

# ========== DATA FILES - DO NOT COMMIT ==========
# Raw data files
data/raw/
!data/raw/.gitkeep  # Keep directory structure

# Large processed files (can be regenerated)
data/processed/*.parquet
data/processed/*.csv
data/processed/*.feather
data/processed/*.h5

# Specific large files
*.txt
*.csv
*.xlsx
*.xls
*.parquet
*.feather
*.hdf5
*.h5
*.pkl
*.pickle

# DVC
.dvc/
.dvcignore

# Logs
logs/
*.log

# Temp
temp/
tmp/

# Project specific
notebooks/__pycache__/
src/__pycache__/

# CI/CD artifacts
.coverage
htmlcov/
.pytest_cache/
.mypy_cache/
EOF